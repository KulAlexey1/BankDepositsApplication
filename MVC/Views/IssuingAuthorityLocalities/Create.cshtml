@model BDA.Domain.IssuingAuthorityLocality

@{
    ViewData["Title"] = "Добавление";
}

<h1>Добавить</h1>

<h4>IssuingAuthorityLocality</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IssuingAuthorityId" class="control-label"></label>
                <select asp-for="IssuingAuthorityId" class ="form-control" asp-items="ViewBag.IssuingAuthorityId"></select>
            </div>
            <div class="form-group">
                <label asp-for="LocalityId" class="control-label"></label>
                <select asp-for="LocalityId" class ="form-control" asp-items="ViewBag.LocalityId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Вернуться к списку</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
